{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST API for Trello Clone",
  "main": "src/server.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:pm2": "cross-env NODE_ENV=production pm2 start src/server.js --watch -i 0",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "lint": "eslint ./ --ignore-path .gitignore --fix",
    "test": "cross-env NODE_ENV=testing jest --no-cache --runInBand --detectOpenHandles --testTimeout=10000 --watch",
    "test:e2e": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace",
    "test:auth": "cross-env NODE_ENV=testing TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [],
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/test/",
      "<rootDir>/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "ajv-errors": "^1.0.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.2",
    "cross-env": "6.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "^4.2.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.16",
    "swagger-ui-express": "4.1.2",
    "uuid": "3.3.3",
    "winston": "^3.3.3",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "debug": "4.1.1",
    "dirty-chai": "2.0.1",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "9.5.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "smtp-server": "^3.8.0",
    "supertest": "^6.0.1"
  }
}
